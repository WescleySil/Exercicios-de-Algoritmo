algoritmo "semnome"
var
   n1, n2, c: inteiro
Procedimento Calc (var a, b, c: inteiro)
Inicio
      Escreva("Digite um valor para ver sua tabuada: ")
      leia(a)
      Para b := 1 ate 10 faca
           c <- a * b
           Escreval(a," x ", b," = ",c)
      FimPara
FimProcedimento
inicio
      Calc(n1, n2, c)
fimalgoritmo
