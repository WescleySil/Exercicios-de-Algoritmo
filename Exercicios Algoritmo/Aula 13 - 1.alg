algoritmo "semnome"
var
   V1, V2, S: INTEIRO
Funcao Soma (a, b: inteiro) : inteiro
inicio
      Retorne a + b
FimFuncao
inicio
      Escreva("Digite o 1.o valor: ")
      leia(V1)
      Escreva("Digite o 2.o valor: ")
      leia(V2)
      S <- Soma(V1, V2)
      Escreva("A soma vale ", S)
fimalgoritmo
